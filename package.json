{
  "name": "@adrianhelvik/fragdom",
  "version": "1.1.2",
  "main": "dist/index.js",
  "module": "src/index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "coveralls": "^3.0.7",
